{
  "name": "reduxible-example",
  "version": "0.0.1-beta",
  "description": "Example of The Universal Web Application based on Reduxible.",
  "scripts": {
    "clean": "rimraf dist",
    "start": "pm2 start app.json",
    "logrotate": "pm2 install pm2-logrotate && pm2 set pm2-logrotate:max_size 1G && pm2 set pm2-logrotate:retain 14",
    "webshell": "pm2 install pm2-webshell && pm2 set pm2-webshell:username coupang && pm2 set pm2-webshell:password admin && pm2 set pm2-webshell:port 9999",
    "stop": "pm2 stop all",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel/register --recursive",
    "check": "npm run lint && npm run test",
    "run:prod": "node dist/server/server.js",
    "run:dev": "concurrent --kill-others \"npm run run:dev:client\" \"npm run run:dev:server\"",
    "run:dev:server": "nodemon --exec babel-node src/server/server.js",
    "run:dev:client": "babel-node webpack/devServer.js",
    "build:server": "npm run clean && babel src --out-dir dist --source-maps",
    "build:client": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/reduxible-example.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^5.8.34",
    "babel-plugin-typecheck": "^2.0.1",
    "babel-runtime": "^5.8.34",
    "compression": "^1.6.0",
    "event-loop-lag": "^1.1.0",
    "express": "^4.13.3",
    "gc-stats": "^1.0.0",
    "graphite": "0.0.6",
    "history": "^1.13.1",
    "json-loader": "^0.5.4",
    "log4js": "^0.6.29",
    "measured": "^1.0.2",
    "pm2": "^0.15.10",
    "purecss": "^0.6.0",
    "query-string": "^3.0.0",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-simple-router": "0.0.8",
    "reduxible": "0.0.2-alpha1",
    "rolex": "0.0.1",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "source-map-support": "^0.3.3",
    "webpack-isomorphic-tools": "^2.2.21"
  },
  "devDependencies": {
    "babel-core": "5.8.34",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.10.0",
    "expect": "^1.13.0",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "rimraf": "^2.4.4",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "^4.2.2"
  }
}
