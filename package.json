{
  "name": "reduxible-example",
  "version": "0.0.2-beta.2",
  "description": "Example of The Universal Web Application based on Reduxible.",
  "scripts": {
    "lint": "eslint src test --fix",
    "test": "mocha",
    "check": "npm run lint && npm run test",
    "clean": "rimraf src/main/webapp",
    "build": "rimraf src/main/webapp && webpack --verbose --colors --display-error-details --config client/webpack/prod.config.js",
    "dev": "concurrent --kill-others \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "npm run clean && NODE_ENV=development node client/webpack/start",
    "dev:server": "./gradlew bootRun",
    "postinstall": "webpack --display-error-details --config client/webpack/prod.config.js",
    "prod": "npm run build && ./gradlew bootRun",
    "heroku": "NODE_ENV=heroku node bin/start",
    "start": "node bin/start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/reduxible-example.git"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-plugin-typecheck": "^2.0.1",
    "babel-runtime": "^5.8.34",
    "body-parser": "^1.14.2",
    "chai": "^3.4.1",
    "classnames": "^2.2.3",
    "compose-dataset-shim": "^1.0.0",
    "compression": "^1.6.0",
    "concurrently": "^1.0.0",
    "console-polyfill": "^0.2.2",
    "cookie-dough": "^0.0.6",
    "cookie-parser": "^1.4.0",
    "cookie-session": "^1.2.0",
    "css-loader": "^0.23.0",
    "es3ify-loader": "^0.1.0",
    "es5-shim": "^4.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.15.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.13.0",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
