{
  "name": "reduxible-example",
  "version": "0.0.2-alpha.11",
  "description": "Example of The Universal Web Application based on Reduxible.",
  "scripts": {
    "clean": "rimraf dist static/dist",
    "lint": "eslint src test",
    "test": "mocha",
    "check": "npm run lint && npm run test",
    "build": "rimraf static/dist && webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "prod": "npm run build && NODE_ENV=production node bin/start",
    "dev": "concurrent --kill-others \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "NODE_ENV=development nodemon bin/start",
    "dev:client": "rimraf webpack*.json && babel-node webpack/devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/reduxible-example.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.7.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.4.0",
    "babel-plugin-typecheck": "^3.6.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.2",
    "compression": "^1.6.0",
    "cookie-dough": "^0.0.6",
    "cookie-parser": "^1.4.0",
    "event-loop-lag": "^1.1.0",
    "express": "^4.13.3",
    "gc-stats": "^1.0.0",
    "graphite": "0.0.6",
    "history": "^1.17.x",
    "json-loader": "^0.5.4",
    "log4js": "^0.6.29",
    "measured": "^1.0.2",
    "purecss": "^0.6.0",
    "query-string": "^3.0.0",
    "react": "^0.14.5",
    "react-addons-css-transition-group": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "redux": "^3.0.4",
    "redux-simple-router": "^1.0.2",
    "reduxible": "0.0.2-alpha.11",
    "rolex": "0.0.1",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "webpack-isomorphic-tools": "^2.2.21"
