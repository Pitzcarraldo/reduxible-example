{
  "name": "reduxible-example",
  "version": "1.0.0",
  "description": "Example of The Universal Web Application based on Reduxible.",
  "main": "src/app/index.js",
  "scripts": {
    "clean": "rimraf dist static/dist",
    "lint": "eslint src test --fix",
    "test": "mocha",
    "check": "npm run lint && npm run test",
    "build": "rimraf static/dist && webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "prod": "NODE_ENV=production npm run build && NODE_ENV=production node bin/start",
    "heroku": "NODE_ENV=heroku node bin/start",
    "start": "NODE_ENV=production node bin/start",
    "dev": "concurrent --kill-others \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "NODE_ENV=development supervisor -e \"js,scss\" bin/start",
    "dev:client": "rimraf webpack*.json && babel-node webpack/devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pitzcarraldo/reduxible-example.git"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "axios": "^0.8.1",
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.34",
    "body-parser": "^1.14.2",
    "chai": "^3.4.1",
    "classnames": "^2.2.3",
    "compression": "^1.6.0",
    "concurrently": "^1.0.0",
    "cookie-dough": "^0.0.6",
    "cookie-parser": "^1.4.0",
    "cookie-session": "^1.2.0",
    "css-loader": "^0.23.0",
    "es3ify-loader": "^0.1.0",
    "eslint": "^2.5.3",
    "eslint-config-reduxible": "reduxible/eslint-config-reduxible",
    "eslint-loader": "^1.3.0",
    "expect": "^1.13.0",
    "express": "^4.13.3",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "history": "1.17.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "monyt": "^0.1.0",
    "node-sass": "^3.4.2",
    "purecss": "^0.6.0",
    "query-string": "^3.0.0",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.5",
    "react-dom": "^0.14.7",
    "react-ga": "^1.2.0",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux": "^3.4.0",
    "redux-promise-middleware": "^3.0.0",
    "redux-provider-middleware": "reduxible/redux-provider-middleware",
    "redux-thunk": "^2.0.1",
    "reduxible": "reduxible/reduxible#1.0.0",
    "reduxible-devtools": "reduxible/reduxible-devtools",
    "rimraf": "^2.4.4",
    "sass-loader": "^3.1.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "supervisor": "^0.9.1",
    "url-loader": "^0.5.7",
    "webpack": "1.12.12",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0",
    "webpack-isomorphic-tools": "^2.2.21",
    "webpack-querify": "^0.0.1"
  },
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  }
}
