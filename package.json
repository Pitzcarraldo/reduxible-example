    "bower": "^1.7.1",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.1",
    "eslint-config-rackt": "^1.1.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.10.0",
    "expect": "^1.13.0",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
